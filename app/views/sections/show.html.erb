


<%= link_to("back to",sections_path) %>
<h2>Section Details </h2>
<table class="section history">

		<tr>
			<th>Name</th>
			<td><%=@section.name %></td>
		</tr>

		<tr>
			<th>Position</th>
			<td><%= @section.position %></td>
		</tr>

		<tr>
			<th>Page_id</th>
			<td><%= @section.page_id %></td>
		</tr>

		<tr>
			<th>Visible </th>
			<td><%= @section.visible ? 'true ': 'false' %></td>
		</tr>
		<tr>
			<th>Content_type</th>
			<td><%= @section.content_type %>
		</tr>
        
        <tr>
			<th>Content</th>
			<td><%= @section.content %>
		</tr>

		
		<tr>
			<th>Created_at</th>
			<td><%= @section.created_at %>
		</tr>

		<tr>
			<th>Updated_at</th>
			<td><%= @section.updated_at %> </td>
		</tr>
</table>